{
  "name": "twitchbuddy",
  "version": "1.0.0",
  "description": "A web app that provides several services for twitch content creators",
  "author": "pierre golfier",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "test:e2e": "cypress open",
    "test:unit:dev": "jest --watch --runInBand",
    "test:unit:ci": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@pedraal/vue-plugin-load-script": "^1.2.0",
    "@sentry/node": "^5.23.0",
    "@sentry/tracing": "^5.23.0",
    "axios": "^0.19.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.4",
    "nuxt": "^2.0.0",
    "vue-handy-ga": "1.2.1",
    "vue-i18n": "^8.17.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/vuetify": "1.9.1",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.1.0",
    "cypress": "^4.9.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^2.6.0",
    "jest": "^26.1.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "vue-jest": "^3.0.5"
  }
}
